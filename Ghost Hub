local MoreScriptSearchTextBox = nil
MoreScriptSearch.Changed:Connect(function()
if MoreScriptSearch.Text ~= MoreScriptSearchTextBox then
   MoreScriptSearchTextBox = MoreScriptSearch.Text
   FrameMoreScript:ClearAllChildren()
   GetMoreScript()
end
end)

MoreScriptSearch:GetPropertyChangedSignal("Text"):Connect(function()
GetMoreScript()
end)

