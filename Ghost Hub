function Main47()
 FreezePlayers = false
 for _, v in pairs(game.Players:GetPlayers()) do 
   if v.Name ~= game.Players.LocalPlayer.Name then   
      v.Character.HumanoidRootPart.Anchored = false
  end
 end
end

