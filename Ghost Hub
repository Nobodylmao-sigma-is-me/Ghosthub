game:GetService("Workspace").LayDownPart:Destroy()
end

function Main43()
 local g = game
 local plrs = g:GetService("Players")
 local lp = plrs.LocalPlayer
 local c = lp.Character

 if c and c.Parent then
    local hum = c:FindFirstChildOfClass("Humanoid")
    if hum then
       hum.Sit = not hum.Sit
    end
 end
end

