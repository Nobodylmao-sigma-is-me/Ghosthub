TextButton14.Parent = Frame
TextButton14.BackgroundColor3 = Color3.new(0,0,0)
TextButton14.BackgroundTransparency = 0
TextButton14.BorderColor3 = Color3.new(1,1,1)
TextButton14.BorderSizePixel = 1
TextButton14.Position = UDim2.new(0.13,0,0.75)
TextButton14.TextColor3 = Color3.new(1,1,1)
TextButton14.Size = UDim2.new(0.3,0,0.12)
TextButton14.Font = Enum.Font.SourceSansLight
TextButton14.FontSize = Enum.FontSize.Size14
TextButton14.Text = "Head"
TextButton14.TextScaled = true
TextButton14.TextSize = 8
TextButton14.TextWrapped = true
TextButton14.Visible = false

TextButton14.MouseButton1Click:connect(function()
local lp = game:GetService "Players".LocalPlayer
if lp.Character:FindFirstChild "Head" then
    local char = lp.Character
    char.Archivable = true
    local new = char:Clone()
    new.Parent = workspace
    lp.Character = new
    wait(2)
    local oldhum = char:FindFirstChildWhichIsA "Humanoid"
    local newhum = oldhum:Clone()
    newhum.Parent = char
    newhum.RequiresNeck = false
    oldhum.Parent = nil
    wait(2)
    lp.Character = char
    new:Destroy()
    wait(1)
    newhum:GetPropertyChangedSignal("Health"):Connect(
        function()
            if newhum.Health <= 0 then
                oldhum.Parent = lp.Character
                wait(1)
                oldhum:Destroy()
            end
        end)
    workspace.CurrentCamera.CameraSubject = char
    if char:FindFirstChild "Animate" then
        char.Animate.Disabled = true
        wait(.1)
        char.Animate.Disabled = false
    end
    lp.Character:FindFirstChild "Head":Destroy()
end
wait(1)
game.Players.LocalPlayer.Character.Head:Destroy()
end)

