Button4.Parent = MainFrame
Button4.BackgroundColor3 = Color3.new(8,0,0)
Button4.BackgroundTransparency = 0
Button4.Position = UDim2.new(0.065, 0, 0.45, 0)
Button4.Size = UDim2.new(0.27,0,0.15)
