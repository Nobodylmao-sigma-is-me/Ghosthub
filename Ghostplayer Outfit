local CoreGui = game:GetService("StarterGui")

CoreGui:SetCore("SendNotification", {
    Title = "Script Made By",
    Text = "GhostPlayer",
    Icon = "http://www.roblox.com/asset/?id=9939435592",
    Duration = 2.5,
})

--Destroy Player Appearance
game.Players.LocalPlayer:ClearCharacterAppearance()
--

--Shirt
local ShirtTexture = "http://www.roblox.com/asset/?id=14658350086"

local CurrentShirt = game.Players.LocalPlayer.Character:findFirstChild("Shirt")
if CurrentShirt ~= nil then
   CurrentShirt.ShirtTemplate = ShirtTexture
else
  local newshirt = Instance.new("Shirt")
  newshirt.Name = "Shirt"
  newshirt.ShirtTemplate = ShirtTexture
  newshirt.Parent = game.Players.LocalPlayer.Character
end
--

--Pants
local PantsTexture = "http://www.roblox.com/asset/?id=6755456435"

local CurrentPants = game.Players.LocalPlayer.Character:findFirstChild("Pants")
if CurrentPants ~= nil then
   CurrentPants.PantsTemplate = PantsTexture
else
  local newpants = Instance.new("Pants")
  newpants.Name = "Pants"
  newpants.PantsTemplate = PantsTexture
  newpants.Parent = game.Players.LocalPlayer.Character
end
--

--Black Horn
plr = game.Players.LocalPlayer
char = plr.Character
char.Archivable = true

hat = Instance.new("Part", char)
hat.Size = Vector3.new(1,1,1)
hatmesh = Instance.new("SpecialMesh", hat)
hatmesh.MeshId = "rbxassetid://8601834297"
hatmesh.Scale = Vector3.new(1, 1 ,1)
hatmesh.TextureId = "rbxassetid://8601834442"
hat.Transparency = 0
hat.CanCollide = false
hat.Name = "Horn"
hat.Anchored = false
hat.BrickColor = BrickColor.new("Really black")
HatWeld = Instance.new("Weld", char)
HatWeld.Part0 = char["Head"]
HatWeld.Part1 = hat
HatWeld.C1 = CFrame.new(0,-1,0)*CFrame.Angles(0,0,0)
--

