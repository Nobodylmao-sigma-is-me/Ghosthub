--Get Player Function
local lplayer = game:GetService('Players').LocalPlayer

function GetPlayer(String)
local Found = {}
local strl = String:lower()
if strl == "all" then
for i,v in pairs(game:GetService("Players"):GetPlayers()) do
table.insert(Found,v)
end
elseif strl == "others" then
for i,v in pairs(game:GetService("Players"):GetPlayers()) do
if v.Name ~= lplayer.Name then
table.insert(Found,v)
end
end 
elseif strl == "me" then
for i,v in pairs(game:GetService("Players"):GetPlayers()) do
if v.Name == lplayer.Name then
table.insert(Found,v)
end
end 
else
for i,v in pairs(game:GetService("Players"):GetPlayers()) do
if v.Name:lower():sub(1, #String) == String:lower() then
table.insert(Found,v)
end
end 
end
return Found 
end
--

--Save Position Function
local function SavePs()
local old = game.Players.LocalPlayer.Character:getChildren()
for i=1,#old do
if old[i].Name == "HumanoidRootPart" then
LastPs = old[i].CFrame
end
end
end

local function TpSavedPs()
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = LastPs
end


TextButton1.Text = "Player Name Here"


TextButton2.Text = "Get Torso"
TextButton2.MouseButton1Click:connect(function()
TextButton1.Text = ""..unpack(GetPlayer(TextButton1.Text)).Name
if game.Players:FindFirstChild(TextButton1.Text) then
SavePs()
for i,v in pairs(workspace:GetDescendants()) do
if v.Name == TextButton1.Text.."/Torso" then
   game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v["Torso"].CFrame
   wait(0.1)
   fireproximityprompt(v["Torso"].ProximityPrompt)
end
end
wait(0.3)
TpSavedPs()
end
end)


