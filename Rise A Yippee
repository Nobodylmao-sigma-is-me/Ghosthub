On3.Parent = Frame
On3.BackgroundColor3 = Color3.new(0,0,0)
On3.BackgroundTransparency = 0
On3.BorderSizePixel = 1
On3.BorderColor3 = Color3.new(0,0,0)
On3.Position = UDim2.new(0.07,0,0.6)
