game.StarterGui:SetCore("ChatMakeSystemMessage", {
Text = "Script Made By GhostPlayer";
Color = Color3.fromRGB(255,250,0);
})

game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0,7,0)

wait()

game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.Angles(math.rad(-260),math.rad(0),math.rad(0))

wait(1)

game.Players.LocalPlayer.Character.Humanoid.RootPart:BreakJoints()

game.Players.LocalPlayer.Character.Animate.Disabled = true
