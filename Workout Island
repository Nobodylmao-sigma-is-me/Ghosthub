local Library = loadstring(Game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wizard"))()

local PhantomForcesWindow = Library:NewWindow("Workout Island")

local Folder1 = PhantomForcesWindow:NewSection("Main")

GemColdown = 370
Folder1:CreateButton("Get Gems", function()
print("HI")
if GemColdown == 370 then
local children = game.Workspace:GetChildren()
	for _, child in pairs(children) do
  		for _, child in pairs(child:GetChildren()) do
       		table.insert(children, child)
  		 end
  		 if child:IsA("BasePart") and child.Name == "GemModel" then
         	 firetouchinterest(child,game.Players.LocalPlayer.Character.HumanoidRootPart,1)
         	 wait(0.1)
         	 firetouchinterest(child,game.Players.LocalPlayer.Character.HumanoidRootPart,0)
end
end
getgenv().Gem = true
while getgenv().Gem == true do
GemColdown -= 1
if GemColdown == 0 then
GemColdown = 370
getgenv().Gem = false
end
wait(1)
end
else
local CoreGui = game:GetService("StarterGui")

CoreGui:SetCore("SendNotification", {
    Title = "Couldown",
    Text = ""..GemColdown,
    Duration = 2.5,
})
end
end)

