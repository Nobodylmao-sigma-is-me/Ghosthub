Folder1:CreateButton("ServerHop", function()
print("HI")
local Http = game:GetService("HttpService")
local TPS = game:GetService("TeleportService")
local Api = "https://games.roblox.com/v1/games/"

local _place = game.PlaceId
local _servers = Api.._place.."/servers/Public?sortOrder=Asc&limit=100"
function ListServers(cursor)
   local Raw = game:HttpGet(_servers .. ((cursor and "&cursor="..cursor) or ""))
   return Http:JSONDecode(Raw)
end

local Server, Next; repeat
   local Servers = ListServers(Next)
   Server = Servers.data[1]
   Next = Servers.nextPageCursor
until Server

TPS:TeleportToPlaceInstance(_place,Server.id,game.Players.LocalPlayer)
end)

local Folder2 = PhantomForcesWindow:NewSection("Auto")

Folder2:CreateButton("Auto Lift", function()
print("HI")
getgenv().autostr = true
while getgenv().autostr == true do
local args = {
[1] = "str"
}
game:GetService("ReplicatedStorage").Remotes.LiftWeight:FireServer(unpack(args))
wait(0.1)
end
end)

Folder2:CreateButton("UnAuto Lift", function()
print("HI")
getgenv().autostr = false
end)

Folder2:CreateButton("Auto Sell", function()
print("HI")
getgenv().autoSell = true
while getgenv().autoSell == true do
local old = game.Players.LocalPlayer.Character:getChildren()
for i=1,#old do
if old[i].Name == "HumanoidRootPart" then
lastPos = old[i].CFrame
end
end
wait()
game.Players.LocalPlayer.Character.Humanoid.RootPart.CFrame = CFrame.new(150, 170, 150)
wait(1)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = lastPos
wait(5)
end
end)

Folder2:CreateButton("UnAuto Sell", function()
print("HI")
getgenv().autoSell = false
end)

Folder2:CreateButton("Auto Protect Territory", function()
print("HI")
getgenv().autoProtect = true
while getgenv().autoProtect == true do
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace:FindFirstChild("RingAreas").Territories.T1.CFrame
wait(1)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace:FindFirstChild("RingAreas").Territories.T2.CFrame
wait(1)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace:FindFirstChild("RingAreas").Territories.T3.CFrame
wait(1)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace:FindFirstChild("RingAreas").Territories.T4.CFrame
wait(1)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace:FindFirstChild("RingAreas").Territories.T5.CFrame
wait(1)
local children = game.Workspace:GetChildren()
	for _, child in pairs(children) do
  		for _, child in pairs(child:GetChildren()) do
       		table.insert(children, child)
  		 end
  		 if child:IsA("BasePart") and child.Name == "KOTHArea" then
         	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = child.CFrame * CFrame.Angles(math.rad(0),math.rad(0),math.rad(90))
		end
  	end
wait(1)
end
end)

Folder2:CreateButton("UnAuto Protect Territory", function()
print("HI")
getgenv().autoProtect = false
end)

Folder2:CreateButton("Auto Get AirDrop", function()
print("HI")
Mode = true
getgenv().autoairdrop = true
while getgenv().autoairdrop == true do
spawn(function()
if workspace:FindFirstChild("Airdrops").Airdrop.Parachute then
if Mode == true then
local old = game.Players.LocalPlayer.Character:getChildren()
for i=1,#old do
if old[i].Name == "HumanoidRootPart" then
lastPs = old[i].CFrame
end
end
Mode = false
end

game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace:FindFirstChild("Airdrops").Airdrop.Parachute.CFrame
wait(3)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = lastPs
else
Mode = true
end
end)
wait(0.1)
end
end)

Folder2:CreateButton("UnAuto Get AirDrop", function()
print("HI")
getgenv().autoairdrop = false
end)

