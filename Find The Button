---------Script Filtering
MainFrame.Text = "Find The Button"


------Locate Buttons function
local function LocateButtons()
for _,v in next, Workspace:GetDescendants() do 
    if v.Name == "button" then
        if not v:FindFirstChild("ButtonGui") then
            local ButtonGui = Instance.new("BillboardGui")
            local Button = Instance.new("TextLabel")

            ButtonGui.Parent = v
            ButtonGui.Name = "ButtonGui"
            ButtonGui.AlwaysOnTop = true
            ButtonGui.LightInfluence = 1
            ButtonGui.Size = UDim2.new(0, 5, 0, 5)
            ButtonGui.StudsOffset = Vector3.new(0, 0, 0)

            Button.Parent = ButtonGui
            Button.BackgroundColor3 = Color3.new(1, 1, 1)
            Button.BackgroundTransparency = 0
            Button.Size = UDim2.new(1, 0, 1, 0)
            Button.Text = ""
            Button.TextColor3 = Color3.new(1, 0, 0)
            Button.TextScaled = true
            Button.Visible = true
        end
    end
end
end
--


------UnLocate Buttons Function
local function UnLocateButtons()
for _,v in next, Workspace:GetDescendants() do 
    if v.Name == "button" then
        if v:FindFirstChild("ButtonGui") then
            v.ButtonGui:Destroy()
        end
    end
end
end
--



Switch1 = false
Button1.Text = "Locate All Button"
Button1.MouseButton1Click:connect(function()
if Switch1 == false then
   Switch1 = true
   LocateButtons()
else
   Switch1 = false
   UnLocateButtons()
end
end)


Button2.Text = "Skip Stage"
Button2.MouseButton1Click:connect(function()
local CurrentTarget = nil
for _,v in next, Workspace:GetDescendants() do 
  if v.Name == "button" then
      local target = v
      if CurrentTarget then
          if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - CurrentTarget.Position).Magnitude > (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - target.Position).Magnitude then
             CurrentTarget = target
          end
      else
          CurrentTarget = target
      end
      fireclickdetector(CurrentTarget.ClickDetector)
  end
end
end)


--Finish Game Operator
AutoFinish = false
game:GetService('RunService').Heartbeat:connect(function()
if AutoFinish == true then
   for _,v in next, Workspace:GetDescendants() do 
     if v.Name == "button" then
        fireclickdetector(v.ClickDetector)
     end
   end
end
end)
--

Button3.Text = "Finish Game"
Button3.MouseButton1Click:connect(function()
AutoFinish = true
wait(0.3)
AutoFinish = false
end)


Button4.Text = "Reset Data"
Button4.MouseButton1Click:connect(function()
game:GetService("ReplicatedStorage").RequestResetData:FireServer()
end)











