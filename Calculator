TextButton15.MouseButton1Click:connect(function()
if String1 == "" and String2 == "" then
else
TextBox.Text = ""
wait(0.2)
AnswerMode = true
ComponentOperator = false
Operator1 = false
Operator2 = true
if Component == 1 then
String1 += String2
TextBox.Text = ""..String1
end
if Component == 2 then
String1 -= String2
TextBox.Text = ""..String1
end
if Component == 3 then
String1 *= String2
TextBox.Text = ""..String1
end
if Component == 4 then
String1 /= String2
TextBox.Text = ""..String1
end
end
end)

